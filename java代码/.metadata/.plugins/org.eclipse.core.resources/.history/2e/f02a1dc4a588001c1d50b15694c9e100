package com.bjpowernode.javase.test001;

public class Test2 {
	
	public static void main(String[] args) {
		// 父类型引用指向子类对象
		Animal a1 = new Cat();
		Animal a2 = new Bird();
		
		if(a1 instanceof Cat) {
			Cat c1 = (Cat)a1;
		}
		if(a1 instanceof Bird) {
			Bird b1 = (Bird)a1;
		}
		
	}

}
