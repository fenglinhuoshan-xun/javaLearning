package com.bjpowernode.javase.test003;

public class FinalTest02 {
	
	// 成员变量之实例变量
	// 实例变量有默认值 + final修饰的变量一旦赋值不能重新赋值
	// 综合考虑，java语言最终规定：实例变量使用final之后，必须手动赋值，不能采用系统默认值
	// final int age; // 编译错误
	
	
	public static void main(String[] args) {
		
		final int a;
		a = 100;
		// 不可二次赋值
		// a = 200;
	}

}
